<h1>Editing week</h1>

<% form_for(@week) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :number %><br />
    <%= f.text_field :number %>
  </p>
  <p>
    <%= f.label :startdate %><br />
    <%= f.datetime_select :startdate %>
  </p>
  <p>
    <%= f.label :enddate %><br />
    <%= f.datetime_select :enddate %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @week %> |
<%= link_to 'Back', weeks_path %>