<h1>Listing managers</h1>

<table>
  <tr>
    <th>Name</th>
	<th>Actions</th>
  </tr>

<% @managers.each do |manager| %>
  <tr>
    <td><%=h manager.display_name %></td>
    <td><%= link_to 'Show', manager %>
    <%= link_to 'Edit', edit_manager_path(manager) %>
    <%= link_to 'Destroy', manager, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New manager', new_manager_path %>